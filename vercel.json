{
  "version": 2,
  "builds": [
    {
      "src": "server/vercel.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**/*",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/vercel.js"
    },
    {
      "src": "/(.*)",
      "dest": "build/$1"
    }
  ]
}
